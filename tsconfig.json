{
    "compilerOptions": {
        "outDir": "./public/",
        "noImplicitAny": true,
        "sourceMap": true,
        "target": "es6",
        "lib": ["es6", "dom", "es2017"],
        "allowJs": true,
        "types": [
            "babylonjs",
            "babylonjs-loaders",
        ],
        "strictNullChecks": true, 
        "module": "amd", 
        "baseUrl": "./",
        "paths": {
            "app/*": ["src/*"]
        }
    }
}